<h3 class="ui header">{{t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.использовано
      class=(if (v-get validationModel "использовано" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.использовано-caption")
      data-test-i-i-s-kursovaya10-расчет-ост-врем-e-использовано=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "использовано" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.доступно
      class=(if (v-get validationModel "доступно" "isInvalid") "error")
      label=(t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.доступно-caption")
      data-test-i-i-s-kursovaya10-расчет-ост-врем-e-доступно=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "доступно" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.расторжДоговора-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.расторжДоговора
      displayAttributeName="дата"
      autocomplete=true
      relationName="расторжДоговора"
      projection="РасторжДоговораL"
      title=(t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.расторжДоговора-caption")
      readonly=readonly
      componentName="расторжДоговораLookup"
      class=(if (v-get validationModel "расторжДоговора" "isInvalid") "error")
      data-test-i-i-s-kursovaya10-расчет-ост-врем-e-расторжДоговора=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "расторжДоговора" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-kursovaya10-расчет-ост-врем-e.оказКонсульт-caption"}}</label>
    {{flexberry-groupedit
      componentName="оказКонсультGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.оказКонсульт
      content=model.оказКонсульт
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "оказКонсульт" "isInvalid") "error")
      data-test-i-i-s-kursovaya10-расчет-ост-врем-e-оказКонсульт=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "оказКонсульт" "message")}}
  </div>
</form>
